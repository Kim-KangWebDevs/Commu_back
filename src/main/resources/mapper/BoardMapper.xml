<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.Commu_back.mapper.BoardMapper">

	<!-- 카테고리 리스트 조회 -->
	<select id="selectCategoryList" resultMap="hashmap">
		<![CDATA[	
			select *  
			from board_category_tb 
			where board_type = #{board_type} 
		]]>
	</select>

	<!-- 카테고리 추가 -->
	<insert id="insertCategory">
		<![CDATA[
			insert 
			into board_category_tb(board_type, board_name) 
			values(#{board_type}, #{board_name}) 
		]]>
	</insert>
	
	<!-- 카테고리 삭제 -->
	<delete id="deleteCategory" parameterType="String">
		<![CDATA[
			delete 
			from board_category_tb 
			where board_type = #{board_type}  
		]]>
	</delete>
	
	<!-- 카테고리 이름 조회 -->
	<select id="selectCategoryName" parameterType="String" resultType="String">
		<![CDATA[
			
		]]>
	</select>
	
	<!-- 게시글 리스트 조회 -->
	<select id="selectBoardList" resultType="com.community.vo.BoardVO">>
		<![CDATA[
			
		]]>
	</select>
	
	<!-- 게시글 조회 -->
	<select id="selectBoard" resultType="com.Commu_back.vo.BoardVO">
		<![CDATA[
			
		]]>
	</select>
	
	<!-- 게시글 추가 -->
	<insert id="insertBoard" parameterType="com.Commu_back.vo.BoardVO">
		<![CDATA[
			
		]]>
	</insert>
	
	<!-- 게시글 삭제 -->
	<delete id="deleteBoard">
		<![CDATA[
			
		]]>
	</delete>

</mapper>